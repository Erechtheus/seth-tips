<?xml version="1.0" encoding="iso-8859-1"?>
<banner-configuration>
  <banner>
    <eval>
      <datasetName>banner.eval.dataset.PubtatorDataset</datasetName>
      <dataset>
        <dataFilename>dnorm-data/DNorm-0.0.7/data/NCBI_disease/NCBI_disease/Corpus.txt</dataFilename>
        <pmidsFilename>dnorm-data/DNorm-0.0.7/data/NCBI_disease/NCBI_disease/NCBI_corpus_test_PMIDs.txt</pmidsFilename>
        <foldType>Disease</foldType>
