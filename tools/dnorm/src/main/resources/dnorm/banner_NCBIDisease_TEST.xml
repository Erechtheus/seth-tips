      <idInputFilename>dnorm-data/DNorm-0.0.7/output/ids.txt</idInputFilename>
      <rawInputFilename>dnorm-data/DNorm-0.0.7/output/raw.txt</rawInputFilename>
      <trainingInputFilename>dnorm-data/DNorm-0.0.7/output/training.txt</trainingInputFilename>
      <outputFilename>dnorm-data/DNorm-0.0.7/output/output.txt</outputFilename>
